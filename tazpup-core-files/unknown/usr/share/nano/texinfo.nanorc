syntax "texinfo" "\.texi$"
header "^\\input texinfo"
magic "Texinfo source text"
comment "@c "
color cyan "^@[a-z]+[[:space:]]+.*$"
color brightmagenta "@[a-z]+\{[^}]*\}"
color yellow "@[a-z]+\{?" "\}"
color brightred "^\*[[:space:]]+.*::.*$"
color green "@c[[:space:]]+.*$"
color ,green "[[:space:]]+$"
